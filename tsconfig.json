{
    "compilerOptions": {
        "allowJs": true,
        "checkJs": true,
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "bundler",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "sourceMap": true,
        "verbatimModuleSyntax": true,
        "noEmit": true,
        "forceConsistentCasingInFileNames": true,
        "noFallthroughCasesInSwitch": true
    }
}