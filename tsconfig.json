{
    "compilerOptions": {
        "allowJs": true,
        "checkJs": true,
        "strict": true,
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "sourceMap": true,
        "verbatimModuleSyntax": true,
        "noEmit": true,
        "forceConsistentCasingInFileNames": true,
        "noFallthroughCasesInSwitch": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noPropertyAccessFromIndexSignature": true
    }
}